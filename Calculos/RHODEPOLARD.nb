Cell[BoxData[
 RowBox[{
  RowBox[{"Polard", "[", 
   RowBox[{"num_", ",", "base_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"n", "=", "num"}], ",", 
      RowBox[{"t", "=", "num"}], ",", 
      RowBox[{"x", "=", "base"}], ",", 
      RowBox[{"y", "=", "base"}], ",", 
      RowBox[{"i", "=", "0"}], ",", 
      RowBox[{"g", "=", "1"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"g", "==", "1"}], ",", 
       RowBox[{
        RowBox[{"i", "+=", "1"}], ";", 
        RowBox[{"x", "=", 
         RowBox[{"Mod", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"x", "^", "2"}], "+", "1"}], ",", "n"}], "]"}]}], ";", 
        RowBox[{"y", "=", 
         RowBox[{"Mod", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"y", "^", "4"}], "+", 
            RowBox[{"2", "*", 
             RowBox[{"y", "^", "2"}]}], "+", "2"}], ",", "n"}], "]"}]}], ";", 
        RowBox[{"g", "=", 
         RowBox[{"GCD", "[", 
          RowBox[{
           RowBox[{"x", "-", "y"}], ",", "n"}], "]"}]}], ";", 
        RowBox[{"Print", "[", 
         RowBox[{"Style", "[", 
          RowBox[{
           RowBox[{"\"\<paso = \>\"", "<>", 
            RowBox[{"ToString", "[", "i", "]"}], "<>", "\"\<, x = \>\"", "<>", 
            RowBox[{"ToString", "[", "x", "]"}], "<>", "\"\<, y = \>\"", "<>", 
            RowBox[{"ToString", "[", "y", "]"}], "<>", "\"\<, mcd = \>\"", "<>", 
            RowBox[{"ToString", "[", "g", "]"}]}], ",", "16"}], "]"}], 
         "]"}]}]}], "]"}], ";", 
     RowBox[{"Return", "[", "g", "]"}]}]}], "]"}]}]], "Input",
