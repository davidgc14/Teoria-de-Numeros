Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"expRapDI", "[", 
    RowBox[{"base_", ",", "exp_", ",", "mod_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"a", "=", "base"}], ",", 
       RowBox[{"m", "=", "mod"}], ",", 
       RowBox[{"b", "=", "exp"}], ",", 
       RowBox[{"acu", "=", "1"}], ",", 
       RowBox[{"i", "=", "1"}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"b", ">", "0"}], ",", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"EvenQ", "[", "b", "]"}], ",", 
           RowBox[{"b", "/=", "2"}], ",", 
           RowBox[{
            RowBox[{"b", "=", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"b", "-", "1"}], ")"}], "/", "2"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"acu", "=", 
             RowBox[{"Mod", "[", 
              RowBox[{
               RowBox[{"acu", "*", "a"}], ",", "m"}], "]"}]}]}]}], "]"}], ";", 
         RowBox[{"a", "=", 
          RowBox[{"PowerMod", "[", 
           RowBox[{"a", ",", "2", ",", "m"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"Print", "[", 
          RowBox[{"Style", "[", 
           RowBox[{
            RowBox[{"\"\<paso = \>\"", "<>", 
             RowBox[{"ToString", "[", "i", "]"}], "<>", "\"\<, exp = \>\"", "<>", 
             RowBox[{"ToString", "[", "b", "]"}], "<>", "\"\<, acu = \>\"", "<>", 
             RowBox[{"ToString", "[", 
              RowBox[{"TraditionalForm", "[", "acu", "]"}], "]"}]}], ",", 
            "16"}], "]"}], "]"}], ";", 
         RowBox[{"i", "++"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "acu", "]"}]}]}], "]"}]}], 
  "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{3.855056547044945*^9},
