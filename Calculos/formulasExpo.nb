Cell[BoxData[
 RowBox[{
  RowBox[{"expRapID", "[", 
   RowBox[{"base_", ",", "exp_", ",", "mod_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"a", "=", "base"}], ",", 
      RowBox[{"m", "=", "mod"}], ",", 
      RowBox[{"digits", "=", 
       RowBox[{"IntegerDigits", "[", 
        RowBox[{"exp", ",", "2"}], "]"}]}], ",", 
      RowBox[{"acu", "=", "1"}], ",", 
      RowBox[{"i", "=", "1"}], ",", "e", ",", 
      RowBox[{"b", "=", "0"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"digits", "!=", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{
        RowBox[{"acu", "=", 
         RowBox[{"PowerMod", "[", 
          RowBox[{"acu", ",", "2", ",", "m"}], "]"}]}], ";", 
        RowBox[{"b", "*=", "2"}], ";", "\[IndentingNewLine]", 
        RowBox[{"e", "=", 
         RowBox[{"First", "[", "digits", "]"}]}], ";", "\[IndentingNewLine]", 
        
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"e", "==", "1"}], ",", 
          RowBox[{
           RowBox[{"acu", "=", 
            RowBox[{"Mod", "[", 
             RowBox[{
              RowBox[{"acu", "*", "a"}], ",", "m"}], "]"}]}], ";", 
           RowBox[{"b", "+=", "1"}], ";"}]}], "]"}], ";", 
        RowBox[{"digits", "=", 
         RowBox[{"Rest", "[", "digits", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{"Style", "[", 
          RowBox[{
           RowBox[{"\"\<paso = \>\"", "<>", 
            RowBox[{"ToString", "[", "i", "]"}], "<>", "\"\<, exp = \>\"", "<>", 
            RowBox[{"ToString", "[", "b", "]"}], "<>", "\"\<, acu = \>\"", "<>", 
            RowBox[{"ToString", "[", 
             RowBox[{"TraditionalForm", "[", "acu", "]"}], "]"}]}], ",", 
           "16"}], "]"}], "]"}], ";", 
        RowBox[{"i", "++"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "acu", "]"}]}]}], "]"}]}]], "Input",
 CellLabel->"In[21]:=",ExpressionUUID->"f4548a8c-36db-4304-8479-853a20cf9b4e"],

